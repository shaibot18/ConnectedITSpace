{
  "name": "ConnectedITSpace",
  "version": "1.1.0",
  "license": "MIT",
  "scripts": {
    "start": "npm-run-all --parallel build",
    "start:prod": "npm-run-all --parallel build:prod",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "clean-build": "rimraf ./build && mkdir build",
    "clean": "npm-run-all clean-dist clean-build",
    "server": "node server.js",
    "server:prod": "cross-env NODE_ENV=production node server.js",
    "prebuild": "npm-run-all clean-dist",
    "build": "webpack --mode=development --config webpack.dev.js",
    "postbuild": "npm run server",
    "prebuild:prod": "npm-run-all clean-build",
    "build:prod": "cross-env NODE_ENV=production webpack --mode=production --config webpack.prod.js",
    "postbuild:prod": "npm run server:prod"
  },
  "dependencies": {
    "@mapbox/mapbox-gl-geocoder": "^2.2.0",
    "@uirouter/angularjs": "^1.0.15",
    "angular": "^1.6.9",
    "bcryptjs": "^2.1.0",
    "body-parser": "^1.13.2",
    "crc": "^3.5.0",
    "cross-env": "^5.1.4",
    "del": "^3.0.0",
    "ejs": "^2.3.3",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "express": "^4.13.0",
    "express-jwt": "^3.0.1",
    "express-session": "^1.11.3",
    "jasmine": "^3.1.0",
    "jsonwebtoken": "^5.0.2",
    "lodash": "^3.10.0",
    "mapbox-gl": "^0.44.1",
    "moment": "^2.22.0",
    "mongodb": "^2.1.16",
    "mongoose": "^4.13.9",
    "npm-run-all": "^4.1.2",
    "q": "^1.4.1",
    "request": "^2.58.0",
    "rimraf": "^2.6.2",
    "rootpath": "^0.1.2",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "underscore": "^1.8.3",
    "webpack-merge": "^4.1.2"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "eslint-plugin-import": "^2.9.0",
    "file-loader": "^1.1.11",
    "gulp": "^3.9.1",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^0.20.3",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14"
  },
  "author": "EvsChen",
  "keywords": []
}
